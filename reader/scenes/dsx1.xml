<dsx>


	<scene root="0" axis_length="1" />

	<views default="0">
		<perspective id="0" near="0" far="20" angle="0">
			<from x="0" y="0" z="0" />
			<to x="20" y="20" z="20" />
		</perspective>
	</views>


	<illumination doublesided="1" local="1" >

		<ambient r="100" g="100" b="100" a="100" />
		<background r="100" g="100" b="100" a="100" />

	</illumination>

	<materials>


		<material id="material1" >
			<emission r="1" g="100" b="100" a="100" />
			<ambient r="1" g="100" b="100" a="100" />
			<diffuse r="1" g="100" b="100" a="100" />
			<specular r="1" g="100" b="100" a="100" />
			<shininess value="5" />
		</material>

	</materials>

	<primitives>

		<primitive id="rectangle 1">
			<rectangle x1="0" y1="0" x2="1" y2="1" />
			<triangle x1="0" y1="0" x2="2" y2="2" />
			<cylinder base="0" top="1" height="1" slices="1" stacks="1" />
			<sphere radius="2" slices="1" stacks="1" />
			<torus inner="5" outer="5" slices="1" loops="1" />

		</primitive>

	</primitives>

	<transformations>
		<transformation id="floorT">
			<translate x="7.5" y="0" z="7.5" />
			<rotate axis="x" angle="-90" />
			<scale x="15" y="15" z="0.2" />
		</transformation>

		<transformation id="LWallT">
			<translate x="0" y="4" z="7.5" />
			<rotate axis="y" angle="-90" />
			<scale x="15" y="8" z="0.2" />
		</transformation>

		<transformation id="PWallT">
			<translate x="7.5" y="4" z="0" />
			<scale x="15" y="8" z="0.2" />
		</transformation>

		<transformation id="TableT">
			<translate x="5" y="0" z="8" />
			<rotate axis="y" angle="-90" />
			<scale x="15" y="8" z="0.2" />
		</transformation>

		<transformation id="TorusT">
			<rotate axis="x" angle="90" />
			<translate x="5" y="4" z="-0.6" />
		</transformation>

		<transformation id="PosterT">
			<rotate axis="y" angle="90" />
			<translate x="-7.5" y="4.5" z="0.1" />
			<scale x="2.5" y="4" z="1" />
		</transformation>

		<transformation id="PyrT">
			<translate x="10" y="0" z="4" />
			<scale x="3" y="3" z="3" />
		</transformation>

		<transformation id="BushT">
			<translate x="5" y="2.2" z="8" />
			<scale x="0.2" y="0.2" z="0.2" />
		</transformation>

		<transformation id="quad1T">
			<translate x="0" y="0" z="0.5" />
		</transformation>

		<transformation id="quad2T">
			<translate x="0" y="0" z="-0.5" />
			<rotate axis="x" angle="-90"/>
			<translate x="0" y="0" z="0.5" />
			<rotate axis="z" angle="-90">

			</transformation>

			<transformation id="quad3T">
				<translate x="0" y="0" z="-1" />
				<rotate axis="y" angle="180"/>
				<rotate axis="z" angle="90"/>
			</transformation>

			<transformation id="quad4T">
				<translate x="0" y="0" z="-0.5" />
				<rotate axis="x" angle="90"/>
				<translate x="0" y="0" z="0.5" />
				<rotate axis="z" angle="270"/>
			</transformation>

			<transformation id="quad5T">
				<translate x="0" y="0" z="-0.5" />
