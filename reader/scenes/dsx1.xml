		</primitive>
		<primitive id="triangulo">
			<triangle x1="0" y1="0" x2="1" y2="1" />
		</primitive>
		<primitive id="cilindro">
			<cylinder base="1" top="1" height="1" slices="100" stacks="10" />
		</primitive>
		<primitive id="esfera">
			<sphere radius="1" slices="100" stacks="100" />
		</primitive>
		<primitive id="torus">
			<torus inner=".75" outer=".7" slices="100" loops="100" />
		</primitive>
	</primitives>

	<transformations>
		<transformation id="floorT">
			<translate x="7.5" y="0" z="7.5" />
			<rotate axis="x" angle="-90" />
			<scale x="15" y="15" z="0.2" />
		</transformation>

		<transformation id="LWallT">
			<translate x="0" y="4" z="7.5" />
			<rotate axis="y" angle="-90" />
