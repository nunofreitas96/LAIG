<dsx> 
	
	
	<scene root="1" axis_length="1" />
	
	<views default="view1" />
	
	<perspective id="p1" near="0" far="20" angle="0">
        <from x="0" y="0" z="0" />
        <to x="20" y="20" z="20" />
    </perspective>
	
	<illumination doublesided="1" local="1" >
    
